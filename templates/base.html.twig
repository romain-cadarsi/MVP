<!DOCTYPE html>
<html lang="fr">

<head>
    {% include('mvp/includes/includeMeta.html.twig') %}

    <style>
        p,a,h1,h2,h3,label,.btn{
            font-family: Cabin !important;
        }
        span{
            font-family: Cabin;
        }
        .imagesParticipations::after{
            content: "75/68";
            position: absolute;
            color: #c6c9cd;
            right: 0;
            top: 10px;
            font-size: 13px;
            font-weight: 400;
        }
    </style>
    {% block css %}
    {% endblock %}
</head>

<body data-icon="6" data-icon-color="#0ee6b0">
<!-- Body Inner -->
<div class="body-inner">
    {% block header %}
        {{ include('mvp/component/header.html.twig') }}
    {% endblock %}

    {% include('mvp/component/search.html.twig') %}

    {% block bodycontent %}
    {% endblock %}



</div>
{% include('mvp/component/footer.html.twig') %}
<!-- end: Body Inner -->
