    <style>
        p,a,h1,h2,h3,label,.btn{
            font-family: Cabin !important;
        }
        span{
            font-family: Cabin;
        }
        .imagesParticipations::after{
            content: "75/68";
            position: absolute;
            color: #c6c9cd;
            right: 0;
            top: 10px;
            font-size: 13px;
            font-weight: 400;
        }
    </style>
    {% block css %}
    {% endblock %}
</head>

<body data-icon="6">
<!-- Body Inner -->
<div class="body-inner">
    {% block header %}
        {{ include('mvp/header.html.twig') }}
    {% endblock %}

    {% block bodycontent %}
    {% endblock %}

</div>
<!-- end: Body Inner -->
<!-- Scroll top -->
<a id="scrollTop"><i class="icon-chevron-up"></i><i class="icon-chevron-up"></i></a>
<!--Plugins-->

<script src="{{ asset('Template/js/jquery.js') }}"></script>
<script src="{{ asset('Template/js/plugins.js') }}"></script>
<!--Template functions-->
<script src="{{ asset('Template/js/functions.js') }}"></script>

<script src="{{ asset('Template/plugins/pageloader/pageloader.js') }}"></script>
<script src="{{ asset('Template/plugins/pageloader/pageloader.init.js') }}"></script>

{% block js %}

{% endblock %}
<script>
    {% for label, messages in app.flashes %}
    {% for message in messages %}
    INSPIRO.elements.notification("{{ message }}","",'{{ label }}')
    {% endfor %}
    {% endfor %}


</script>
<script src="//code.tidio.co/7qoxrarewrghzq8qhtvue4dq1nfh4v9y.js" async></script>
